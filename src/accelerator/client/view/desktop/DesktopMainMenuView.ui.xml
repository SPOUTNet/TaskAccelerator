<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:v="urn:import:accelerator.client.view" xmlns:u="urn:import:accelerator.client.ui.widget">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:DockLayoutPanel styleName="mainmenuview">
		<g:center>
			<g:ScrollPanel>
				<c:CellTree ui:field="mainMenu"></c:CellTree>
			</g:ScrollPanel>
		</g:center>
		<g:south size="20">
			<g:HorizontalPanel width="100%" height="20px" styleName="mainmenuview-footer">
				<g:cell verticalAlignment="ALIGN_MIDDLE">
					<g:HorizontalPanel height="20px">
						<u:IconButton ui:field="createButton" icon="ui-icon-plus"
							title="リストの作成" tabIndex="1" />
						<u:IconButton ui:field="editButton" title="リストの編集"
							icon="ui-icon-pencil" tabIndex="2" />
					</g:HorizontalPanel>
				</g:cell>
			</g:HorizontalPanel>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder> 