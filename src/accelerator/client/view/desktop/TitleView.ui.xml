<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:u="urn:import:accelerator.client.ui.widget">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:DockLayoutPanel>
		<g:center>
			<g:HorizontalPanel width="100%" height="100%"
				styleName="titleview-header">
				<g:cell verticalAlignment="ALIGN_MIDDLE">
					<g:Label ui:field="titleLabel">TaskAccelerator</g:Label>
				</g:cell>
				<g:cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_RIGHT">
					<g:Anchor ui:field="authAnchor">Login</g:Anchor>
				</g:cell>
			</g:HorizontalPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 